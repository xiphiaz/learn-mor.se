{
    "require": {
        "meenie/Munee": "*",
        "jquery/jquery": "*",
        "twitter/bootstrap": "*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.10.0",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.10.0.min.js",
                    "type": "file"
                }
            }
        },
        {
       "type": "package",
       "package": {
           "version": "2.2.1",
